function CheckcontactsendOTP(userphone, usertype, strflag, action, frmtype="",product_id=''){
  jQuery.ajax({
    url: "https://agribegri.com/API/api_contact_login_check.php",
    type:'POST',
    dataType: 'json',
    data:{userphone:userphone,type:usertype,strflag:strflag,action:action,product_id:product_id},
    success:function(data){         
      if(data.response_code=="0"){
        jAlert(data.response_error);
        return false;
      }else if(data.response_code=="1"){
        //jAlert(data.response_data);
        jQuery("#spContactresendmessage").hide();
        jQuery("#spContactwaitmessage").hide();
        jQuery("#spContactmessage").show();
        jQuery("#lblcontactOTP").show();
        jQuery("#hidIscontactOtpSent").val('1');
        jQuery("#spContactmessage").html(data.response_data + " If you didn't receive SMS then wait <span id='divmsgCounter' style='color: #000000 !important;'>"+ valcounter +"</span> seconds.");
        sessionStorage.clear();
        sessionStorage.setItem("countersms", 0);
        sessionStorage.removeItem("countersms");
        var cntvalue = valcounter;
        SessionFunction(strflag,action,frmtype);
        
        return false;
      }
    }
  });
  
}

