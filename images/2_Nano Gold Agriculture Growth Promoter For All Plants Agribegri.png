						  ajax_url = 'https://agribegri.com/add_to_cart.php';
	
						  cart_url = 'https://agribegri.com/my_cart.php';
	
						}
	
						var temp_order_id = jQuery('#temp_order_id').val();
	
						jQuery.ajax({
	
						  url:ajax_url,
	
						  data:{item_id:id,price:price,temp_order_id:temp_order_id,qty:jQuery('#french-hens').val(),saller_id:saller_id,visitor_mobile:visitor_mobile,abpdv_id:abpdv_id,product_color:product_color,product_title_arr:product_title_arr,product_unit_title_arr:product_unit_title_arr},
	
