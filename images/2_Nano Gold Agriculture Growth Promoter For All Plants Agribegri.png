function Showtime(){

    var element = jQuery(this);

    var offer_date = $('.offrer_till_date').val();

    var offer_time = $('.offrer_till_time').val();

    if(offer_date == null || offer_time == null)
    {
      var offer_date = '0000-00-00';
      var offer_time = '00:00';
    }

    var split_date = offer_date.split('-');

    var split_time = offer_time.split(':'); 

    var timer_month = split_date[1];

    var timer_day = split_date[2];

    var timer_year = split_date[0];

    var timer_hour = split_time[0];

    var timer_min = split_time[1];

    if(timer_hour == "")

      timer_hour = 0;

      

    if(timer_min =="")

      timer_min=0;

    var timer_date=timer_month+"/"+timer_day+"/"+timer_year+" "+timer_hour+":"+timer_min;

    var end = new Date(timer_date); // Arrange values in Date Time Format

    var now = new Date(); // Get Current date time

    var second = 1000; // Total Millisecond In One Sec

    var minute = second * 60; // Total Sec In One Min

    var hour = minute * 60; // Total Min In One Hour

    var day = hour * 24; // Total Hour In One Day

    

    var now = new Date();

    var remain = end - now; // Get The Difference Between Current and entered date time

    

    if(remain < 0){  

      element.find(".timer_value_desc").html('');

      $(".timer_value_desc").hide();

    }else if(element.find('.offrer_till_date').val() != '0000-00-00' && element.find('.offrer_till_time').val() != ""){

      var days = Math.floor(remain / day); 

      var hours = Math.floor((remain % day) / hour); 

      var minutes = Math.floor((remain % hour) / minute); 

      var seconds = Math.floor((remain % minute) / second); 



      if(hours < 10){

        hours = '0'+hours;

      }

      if(minutes < 10){

        minutes = '0' + minutes;  

      }

      if(seconds < 10){

        seconds = '0' + seconds;  

      }

      $(".timer_value_desc .hours .val").text(hours);

      $(".timer_value_desc .minutes .val").text(minutes);

      $(".timer_value_desc .seconds .val").text(seconds);

    }else{

      $(".timer_value_desc").hide();

    }
}//Showtime

