function validate_contact_login(strflag){
  var isValid = true;
  if(jQuery.trim(jQuery('#abu_contact_login_modal').val())==""){
      jQuery('#abu_contact_login_modal').css({'border-color':'red'});
      isValid = false;
  }else{
      jQuery('#abu_contact_login_modal').attr('style','');
  }
  if(isValid){
      
      if(strflag=='resend'){jQuery("#hidIscontactOtpSent").val('0');}
      if(jQuery("#hidIscontactOtpSent").val()=="1"){
          jQuery('#abu_contact_user_login_modal').attr('style','');
          if(jQuery("#abu_contact_user_login_modal").val()==""){
              jQuery('#abu_contact_user_login_modal').css({'border-color':'red'});
          }else{
              VerifyContactOTP(jQuery('#abu_contact_login_modal').val(),'contact',jQuery("#abu_otp_contact_login_modal").val(),'userlogin','contactpopup',jQuery("#product_id").val());
          }
      }else{
          CheckcontactsendOTP(jQuery('#abu_contact_login_modal').val(),'contact',strflag,'contactcheck','userloginpopup',jQuery("#product_id").val());
      }
      return false;
  }else{
      return false;
  }   
}

